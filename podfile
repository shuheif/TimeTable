end

post_install do | installer |
  require 'fileutils'
  FileUtils.cp_r('Pods/Target Support Files/Pods-TimeTable/Pods-TimeTable-acknowledgements.plist', 'TimeTable/Settings.bundle/Acknowledgements.plist', :remove_destination => true)
end
